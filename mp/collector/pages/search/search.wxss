page {
  background-color: #dedede;
  padding: 20rpx;
}

.search_input {
  display: flex;
  margin-bottom: 30rpx;
  height: 80rpx;
}
.search_input input {
  flex: 1;
  height: 100%;
  padding-left: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.search_content view {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.search_content .active {
  background-color: #ccc;
}