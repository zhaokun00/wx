page {
  background-color: #dedede;
}

.title {
  width: 100%;
  background-color: var(--themeColor);
  padding: 10rpx;
}

.tit_seabox{
  height: 60rpx;
  padding:10rpx 32rpx;
  border-radius: 30rpx;
  background: white;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.tit_seabox_bar icon{
  margin-right: 20rpx;
}

.tit_seabox input{
  height:60rpx;
  line-height:60rpx;
  font-size:28rpx;
  width:100%;
  margin-right:32rpx;
}

.content {
  height: calc(100vh - 80rpx);
  overflow-y:auto;
}

.search_content view {
  background-color: #fff;
  font-size: 30rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.search_content .active {
  background-color: #ccc;
}